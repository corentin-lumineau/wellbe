<div id="navbar_global">
  <%= link_to "WELLBE", sessions_choice_path  %>
  <div class="icone">
    <%= link_to current_user do %>
      <% if current_user%>
      <div class="flex-b-c">
        <div class="avatar-navbar">
          <%= current_user.first_name %>
        </div>
        <%= cl_image_tag current_user.avatar.key, class: 'avatar' %>
      </div>
      <% else %>
      <% cl_image_tag "lourd.jpg" %>
      <% end %>
    <% end %>
    <%= link_to users_path do %>
      <div class="icone-style">
        <i class="fas fa-user-friends"></i>
      </div>
    <% end %>
    <div class="icone-style">
      <i class="fas fa-comments"></i>
    </div>
    <div class="icone-style">
      <i class="fas fa-tools"></i>
    </div>
  </div>
</div>
<div class="trace-gradiant"></div>
